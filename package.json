    "test:update-baseline": "bun run tests/run-tests.ts --visual --update-baseline",
    "test:ci": "bun run tests/run-tests.ts --unit --integration --e2e --coverage",
    "test:smoke": "bun test tests/smoke",
    "precommit": "bun run check && bun run typecheck && bun run test:unit",
    "healthcheck": "bun run scripts/healthcheck.js",
    "deploy": "./scripts/deploy.sh",
    "setup:branch-protection": "./scripts/setup-branch-protection.sh",
    "setup:worktrees": "./scripts/setup-worktrees.sh",
    "wt:create": "./scripts/worktree-create.sh",
    "wt:switch": "./scripts/worktree-switch.sh",
    "wt:sync": "./scripts/worktree-sync.sh",
    "wt:merge": "./scripts/worktree-merge.sh",
    "wt:clean": "./scripts/worktree-cleanup.sh",
    "wt:health": "./scripts/worktree-health.sh"
